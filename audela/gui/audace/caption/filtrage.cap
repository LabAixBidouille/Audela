   set caption(filtrage,tracer)             "Porfavor, dibuje un recuadro con el ratón."

# *************** Version allemande ***************************
} elseif { [ string compare $langage "german" ] == "0" } {

   set caption(filtrage,chargement)         "Datei laden:"
