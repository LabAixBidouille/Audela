   set caption(filtrage,tracer)             "Por favor desenhe um quadrado com o rato."

# *************** Version danoise *****************************
} elseif { [ string compare $langage "danish" ] == "0" } {

   set caption(filtrage,chargement)         "Indlæs fil:"
