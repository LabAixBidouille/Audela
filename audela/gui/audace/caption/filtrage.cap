global caption langage

# *************** Version anglaise ****************************

set caption(filtrage,chargement)            "Loading File:"
set caption(filtrage,fin_traitement)        "End of Processing."
set caption(filtrage,pas_de_fichier)        "File Does Not Exist.\n"
set caption(filtrage,charge_image)          "An image must be loaded.\n"
set caption(filtrage,attention)             "Warning"
set caption(filtrage,pas_image_memoire)     "No Image in Memory."
set caption(filtrage,tracer)                "Please draw a square with your mouse."

# *************** Version française ***************************
if { [ string compare $langage "french" ] == "0" } {

   set caption(filtrage,chargement)         "Chargement du fichier :"
   set caption(filtrage,fin_traitement)     "Fin du traitement."
   set caption(filtrage,pas_de_fichier)     "Le fichier n'existe pas.\n"
   set caption(filtrage,charge_image)       "Il faut charger une image.\n"
   set caption(filtrage,attention)          "Attention"
   set caption(filtrage,pas_image_memoire)  "Il n'y a pas d'image actuellement en mémoire."
   set caption(filtrage,tracer)             "Veuillez tracer un cadre à la souris."

# *************** Version italienne ***************************
} elseif { [ string compare $langage "italian" ] == "0" } {

   set caption(filtrage,chargement)         "Caricamento del file:"
   set caption(filtrage,fin_traitement)     "Fine del elaborazione."
   set caption(filtrage,pas_de_fichier)     "Il file non esiste.\n"
   set caption(filtrage,charge_image)       "Deve essere caricata un'immagine.\n"
   set caption(filtrage,attention)          "Attenzione"
   set caption(filtrage,pas_image_memoire)  "Nessuna immagine in memoria."
   set caption(filtrage,tracer)             "Disegnare un quadrato con il mouse."

# *************** Version espagnole ***************************
} elseif { [ string compare $langage "spanish" ] == "0" } {

   set caption(filtrage,chargement)         "Cargando fichero:"
