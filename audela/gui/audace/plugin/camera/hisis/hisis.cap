#=========================================================================
#   Début de la déclaration des textes localisés (internationalisation)
#=========================================================================

global caption langage

# *************** Version anglaise ****************************

set caption(hisis,camera)                       "Hi-SIS"

set caption(hisis,obtu_ouvert)                  "Open"
set caption(hisis,obtu_ferme)                   "Closed"
set caption(hisis,obtu_synchro)                 "Synchro"

# *************** Version française ***************************
if { [ string compare $langage "french" ] == "0" } {

   set caption(hisis,camera)                    "Hi-SIS"

   set caption(hisis,obtu_ouvert)               "Ouvert"
   set caption(hisis,obtu_ferme)                "Fermé"
   set caption(hisis,obtu_synchro)              "Synchro"

# *************** Version italienne ***************************
} elseif { [ string compare $langage "italian" ] == "0" } {

   set caption(hisis,camera)                    "Hi-SIS"

   set caption(hisis,obtu_ouvert)               "Aperto"
   set caption(hisis,obtu_ferme)                "Chiuso"
   set caption(hisis,obtu_synchro)              "Sincronizza"

# *************** Version espagnole ***************************
} elseif { [ string compare $langage "spanish" ] == "0" } {

   set caption(hisis,camera)                    "Hi-SIS"

   set caption(hisis,obtu_ouvert)               "Abierto"
   set caption(hisis,obtu_ferme)                "Cerrado"
