#=========================================================================
#   Début de la déclaration des textes localisés (internationalisation)
#=========================================================================

global caption langage

# *************** Version anglaise ****************************

set caption(serialport,titre)           "Serial Port"
set caption(serialport,available)       "Serial Ports Available"
set caption(serialport,used)            "Used Serial Ports"
set caption(serialport,refresh)         "Reload"
set caption(serialport,port_exclus)     "COM port to be excluded (internal modem, etc.)"

# *************** Version française ***************************
if { [ string compare $langage "french" ] == "0" } {

   set caption(serialport,titre)        "Port série"
   set caption(serialport,available)    "Ports séries disponibles"
   set caption(serialport,used)         "Ports séries utilisés"
   set caption(serialport,refresh)      "Rafraîchir"
   set caption(serialport,port_exclus)  "Port COM à exclure (modem interne, etc.)"

# *************** Version italienne ***************************
} elseif { [ string compare $langage "italian" ] == "0" } {

   set caption(serialport,titre)        "Porta seriale"
   set caption(serialport,available)    "Porta seriale disponibile"
   set caption(serialport,used)         "# Ports séries utilisés"
   set caption(serialport,refresh)      "Rinfresca"
   set caption(serialport,port_exclus)  "Porta COM da escludere (modem interno, ecc.)"

# *************** Version espagnole ***************************
} elseif { [ string compare $langage "spanish" ] == "0" } {

   set caption(serialport,titre)        "Puerto serie"
   set caption(serialport,available)    "Puertos serie disponibles"
   set caption(serialport,used)         "# Ports séries utilisés"
   set caption(serialport,refresh)      "Recargar"
   set caption(serialport,port_exclus)  "Puerto COM a excluir (modem interno, etc.)"

# *************** Version allemande ***************************
} elseif { [ string compare $langage "german" ] == "0" } {

   set caption(serialport,titre)        "Serial Anschlüss"
   set caption(serialport,available)    "Serial Anschlüssen verfügbar"
   set caption(serialport,used)         "# Ports séries utilisés"
   set caption(serialport,refresh)      "Aktualisieren"
   set caption(serialport,port_exclus)  "COM Port zu ausschließen (inneren Modem, etc.)"

# *************** Version portugaise **************************
} elseif { [ string compare $langage "portuguese" ] == "0" } {

   set caption(serialport,titre)        "Porta série"
   set caption(serialport,available)    "Portas série disponiveis"
   set caption(serialport,used)         "# Ports séries utilisés"
   set caption(serialport,refresh)      "Actualizar"
   set caption(serialport,port_exclus)  "Porta COM a excluir (modem interno, etc.)"

# *************** Version danoise *****************************
} elseif { [ string compare $langage "danish" ] == "0" } {

   set caption(serialport,titre)        "Seriel port"
   set caption(serialport,available)    "Serielle porte disponible"
   set caption(serialport,used)         "# Ports séries utilisés"
   set caption(serialport,refresh)      "Genindlæs"
   set caption(serialport,port_exclus)  "COM-port som fravælges (indbygget modem osv.)"

}

#=========================================================================
#   Fin de la déclaration des textes localisés (internationalisation)
#=========================================================================

