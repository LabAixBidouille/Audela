global caption langage

# *************** Version anglaise ****************************

set caption(convert,menu)                 "Convert the displayed FITS image"
set caption(convert,titre)                "Convert the displayed FITS image"
set caption(convert,erreur)               "Error"

# *************** Version française ***************************
if { [ string compare $langage "french" ] == "0" } {

   set caption(convert,menu)              "Convertir l'image FITS affichée"
   set caption(convert,titre)             "Convertir l'image FITS affichée"
   set caption(convert,erreur)            "Erreur"

# *************** Version italienne ***************************
} elseif { [ string compare $langage "italian" ] == "0" } {

   set caption(convert,menu)              "Convertire l'immagine FITS visualizzata"
   set caption(convert,titre)             "Convertire l'immagine FITS visualizzata"
   set caption(convert,erreur)            "Errore"

# *************** Version espagnole ***************************
} elseif { [ string compare $langage "spanish" ] == "0" } {

   set caption(convert,menu)              "Convertir la imagen FITS presentada"
   set caption(convert,titre)             "Convertir la imagen FITS presentada"
   set caption(convert,erreur)            "Error"

# *************** Version allemande ***************************
} elseif { [ string compare $langage "german" ] == "0" } {

   set caption(convert,menu)              "Das gezeigte FITS Bild konvertieren"
   set caption(convert,titre)             "Das gezeigte FITS Bild konvertieren"
