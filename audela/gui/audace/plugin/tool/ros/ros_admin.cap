set caption(ros_admin,efface)             "Data base will be deleted"
set caption(ros_admin,mdprootsql)         "Enter the root password for the SQL server"
set caption(ros_admin,cancelRAZ)          "DB reset canceled"
set caption(ros_admin,mysqlconnecterr)    "MySQL connection error:"
set caption(ros_admin,mysqlshowerr)       "Request failed:"
set caption(ros_admin,mysqlok)            "Connecting to DB:\nSuccess"

