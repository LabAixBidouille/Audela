   set caption(parallelport,refresh)      "# Rafraîchir"

# *************** Version espagnole ***************************
} elseif { [ string compare $langage "spanish" ] == "0" } {

   set caption(parallelport,titre)        "Puerto paralelo"
   set caption(parallelport,available)    "# Ports parallèles disponibles"
   set caption(parallelport,refresh)      "# Rafraîchir"

# *************** Version allemande ***************************
} elseif { [ string compare $langage "german" ] == "0" } {

   set caption(parallelport,titre)        "Parallele Schnittstelle"
   set caption(parallelport,available)    "# Ports parallèles disponibles"
