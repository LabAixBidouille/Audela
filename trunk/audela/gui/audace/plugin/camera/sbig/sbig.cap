#=========================================================================
#   Début de la déclaration des textes localisés (internationalisation)
#=========================================================================

global caption langage

# *************** Version anglaise ****************************

set caption(sbig,camera)                       "SBIG"

set caption(sbig,obtu_ouvert)                  "Open"
set caption(sbig,obtu_ferme)                   "Closed"
set caption(sbig,obtu_synchro)                 "Synchro"

# *************** Version française ***************************
if { [ string compare $langage "french" ] == "0" } {

   set caption(sbig,camera)                    "SBIG"

   set caption(sbig,obtu_ouvert)               "Ouvert"
   set caption(sbig,obtu_ferme)                "Fermé"
   set caption(sbig,obtu_synchro)              "Synchro"

# *************** Version italienne ***************************
} elseif { [ string compare $langage "italian" ] == "0" } {

   set caption(sbig,camera)                    "SBIG"

   set caption(sbig,obtu_ouvert)               "Aperto"
   set caption(sbig,obtu_ferme)                "Chiuso"
   set caption(sbig,obtu_synchro)              "Sincronizza"

# *************** Version espagnole ***************************
} elseif { [ string compare $langage "spanish" ] == "0" } {

   set caption(sbig,camera)                    "SBIG"

   set caption(sbig,obtu_ouvert)               "Abierto"
   set caption(sbig,obtu_ferme)                "Cerrado"
   set caption(sbig,obtu_synchro)              "Sincro"

# *************** Version allemande ***************************
} elseif { [ string compare $langage "german" ] == "0" } {

   set caption(sbig,camera)                    "SBIG"

   set caption(sbig,obtu_ouvert)               "Offen"
   set caption(sbig,obtu_ferme)                "Geschlossen"
   set caption(sbig,obtu_synchro)              "Synchro"

# *************** Version portugaise ***************************
} elseif { [ string compare $langage "portuguese" ] == "0" } {

   set caption(sbig,camera)                    "SBIG"

   set caption(sbig,obtu_ouvert)               "Aberto"
   set caption(sbig,obtu_ferme)                "Fechado"
   set caption(sbig,obtu_synchro)              "Sincro"

# *************** Version danoise *****************************
} elseif { [ string compare $langage "danish" ] == "0" } {

   set caption(sbig,camera)                    "SBIG"

   set caption(sbig,obtu_ouvert)               "Open"
   set caption(sbig,obtu_ferme)                "Closed"
   set caption(sbig,obtu_synchro)              "Synchro"

}

#=========================================================================
#   Fin de la déclaration des textes localisés (internationalisation)
