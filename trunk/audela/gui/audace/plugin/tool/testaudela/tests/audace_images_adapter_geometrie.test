   ::testaudela::clicButton $w.cmd.appliquer

   #--   procedure de controle
   set result 0
   if {[lindex [buf$bufNo getkwd NAXIS1] 1] == "120"} {incr result}
   if {[lindex [buf$bufNo getkwd NAXIS2] 1] == "130"} {incr result}
