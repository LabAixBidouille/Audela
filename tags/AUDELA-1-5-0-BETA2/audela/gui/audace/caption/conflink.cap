global caption langage

# *************** Version anglaise ****************************

set caption(conflink,liaison)                "Link:"
set caption(conflink,config)                 "Link Configuration"
set caption(conflink,creer_au_demarrage)     "Next time software is run connect this link automatically"
set caption(conflink,ok)                     "OK"
set caption(conflink,appliquer)              "Apply"
set caption(conflink,fermer)                 "Close"
set caption(conflink,aide)                   "Help"

set caption(conflink,pas_liaison)            "No Link."

# *************** Version française ***************************
if { [ string compare $langage "french" ] == "0" } {

   set caption(conflink,liaison)             "Liaison :"
   set caption(conflink,config)              "Configuration de la liaison"
